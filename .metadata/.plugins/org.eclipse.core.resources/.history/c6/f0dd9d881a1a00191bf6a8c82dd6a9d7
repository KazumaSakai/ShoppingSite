package com.internousdev.ShoppingSite.action;

import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.internousdev.ShoppingSite.dao.AddItemDAO;
import com.opensymphony.xwork2.ActionSupport;

public class ItemPageAction extends ActionSupport implements SessionAware
{
	private int item_id;
	private Map<String, Object> session;

	public String execute()
	{

	}

	public void setRequest_Quantity(int request_Quantity) {
		this.request_Quantity = request_Quantity;
	}

	@Override
	public void setSession(Map<String, Object> session) {
		this.session = session;
	}
}
