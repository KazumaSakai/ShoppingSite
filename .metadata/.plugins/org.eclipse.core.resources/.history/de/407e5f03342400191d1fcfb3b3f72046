package com.internousdev.ShoppingSite.util;

public class SessionSafeGetter
{
	public static String getString(String key)
	{
		Object value = session.get("LoginedRedirectAction");
		
		if(loginedRedirectAction != null)
		{
			String string = (String)loginedRedirectAction;
			session.put("LoginedRedirectAction", "");
		}
	}
}
