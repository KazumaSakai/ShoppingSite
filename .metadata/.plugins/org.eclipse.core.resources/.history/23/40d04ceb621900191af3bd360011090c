	public boolean addItemToCart(int item_id, int user_id)
	{
		String sql = "SELECT count(*) FROM carts WHERE item_id = ? AND user_id = ?";

		try
		{
			PreparedStatement preparedStatement = connection.prepareStatement(sql);
		}

		return false;
	}
}
